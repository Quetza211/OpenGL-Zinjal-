void init(){
	glClearColor(1.0,1.0,1.0,0.0);
	glMatrixMode(GL_PROJECTION);
	gluOrtho2D(-400.0,400.0,-400.0,400.00);
}
	void Punto1(){
		glClear(GL_COLOR_BUFFER_BIT);
		glColor3f(1.0,0.0,0.0);
		glLineWidth(2.0);
		glBegin(GL_LINE_LOOP);
		for(int i=0;i<sizeof figura1/sizeof figura1[0];i+=2){
			glVertex2f(figura1[i],figura1[i+1]);
		}
		glEnd();
		glFlush();
	}
		void Punto2 (){
